{
  "name": "angular-content-portal",
  "version": "7.1.3",
  "description": "A simple library that uses the @angular/cdk library to give a quick way of portaling(?) content outside of your component.",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnCashmore/angular-content-portal"
  },
  "author": {
    "name": "John Cashmore",
    "email": "john@cashmo.re"
  },
  "bugs": {
    "url": "https://github.com/JohnCashmore/angular-content-portal/issues"
  },
  "keywords": [
    "angular",
    "portal",
    "cdk",
    "projection"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest --coverage",
    "test-ci": "jest --coverage --collectCoverageFrom=src/app/modules/portal/*.ts",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "pub": "ng-packagr -p ng-package.json && npm publish dist",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch",
    "precise-commits": "precise-commits"
  },
  "coverageReporters": [
    "text",
    "cobertura",
    "lcov"
  ],
  "dependencies": {
    "@angular/animations": "7.2.5",
    "@angular/cdk": "7.3.2",
    "@angular/common": "7.2.5",
    "@angular/compiler": "7.2.5",
    "@angular/core": "7.2.5",
    "@angular/forms": "7.2.5",
    "@angular/http": "7.2.5",
    "@angular/platform-browser": "7.2.5",
    "@angular/platform-browser-dynamic": "7.2.5",
    "@angular/router": "7.2.5",
    "@briebug/jest": "1.3.1",
    "core-js": "2.6.5",
    "rxjs": "6.4.0",
    "rxjs-compat": "6.4.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.1",
    "@angular/cli": "7.3.1",
    "@angular/compiler-cli": "7.2.5",
    "@angular/language-service": "7.2.5",
    "@types/jasmine": "3.3.9",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "9.6.42",
    "codelyzer": "4.5.0",
    "coveralls": "3.0.2",
    "husky": "^1.3.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.1.0",
    "jest-preset-angular": "6.0.2",
    "karma-phantomjs-launcher": "1.0.4",
    "ng-packagr": "4.7.1",
    "precise-commits": "^1.0.2",
    "prettier": "^1.16.4",
    "protractor": "5.4.2",
    "ts-node": "8.0.2",
    "tsickle": "0.34.3",
    "tslint": "5.12.1",
    "typescript": "3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precise-commits"
    }
  }
}
