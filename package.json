{
  "name": "angular-content-portal",
  "version": "1.0.0",
  "description": "A simple library that uses the @angular/cdk library to give a quick way of portaling(?) content outside of your component.",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnCashmore/angular-content-portal"
  },
  "author": {
    "name": "John Cashmore",
    "email": "john@cashmo.re"
  },
  "bugs": {
    "url": "https://github.com/JohnCashmore/angular-content-portal/issues"
  },
  "keywords": [
    "angular",
    "portal",
    "cdk",
    "projection"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest --coverage",
    "test-ci": "jest --coverage --collectCoverageFrom=src/app/modules/portal/*.ts",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "pub": "ng-packagr -p ng-package.json && npm publish dist",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch"
  },
  "coverageReporters": [
    "text",
    "cobertura",
    "lcov"
  ],
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/cdk": "6.3.3",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@briebug/jest": "1.3.0",
    "core-js": "2.5.7",
    "rxjs": "6.2.1",
    "rxjs-compat": "6.2.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.6.22",
    "codelyzer": "4.4.2",
    "coveralls": "3.0.2",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "23.3.0",
    "jest-preset-angular": "5.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "ng-packagr": "3.0.3",
    "protractor": "5.3.2",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.7.2"
  }
}
