{
  "name": "angular-content-portal",
  "version": "0.3.6",
  "description": "A simple library that uses the @angular/cdk library to give a quick way of portaling(?) content outside of your component.",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnCashmore/angular-content-portal"
  },
  "author": {
    "name": "John Cashmore",
    "email": "john@cashmo.re"
  },
  "bugs": {
    "url": "https://github.com/JohnCashmore/angular-content-portal/issues"
  },
  "keywords": ["angular", "portal", "cdk", "projection"],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test --watch=false --code-coverage --browser=PhantomJS",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "pub": "ng-packagr -p ng-package.json && npm publish dist",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },

  "dependencies": {
    "@angular/cdk": "5.2.5",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "5.2.10",
    "@angular/animations": "5.2.10",
    "core-js": "2.5.5",
    "rxjs": "5.5.10",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.10",
    "@angular/language-service": "5.2.10",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.6.6",
    "codelyzer": "4.3.0",
    "coveralls": "3.0.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "ng-packagr": "2.4.2",
    "protractor": "5.3.1",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  }
}
